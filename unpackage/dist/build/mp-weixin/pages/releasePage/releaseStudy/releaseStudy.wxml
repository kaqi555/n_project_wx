<view class="release"><view class="head"><image src="{{headPic}}" mode></image><text>{{username}}</text></view><view class="studyForm"><form data-event-opts="{{[['submit',[['releaseStudy',['$event']]]],['reset',[['',['$event']]]]]}}" bindsubmit="__e" bindreset="__e"><view class="studyInput"><view class="title">资料名:</view><input type="text" value="" name="studyName"/></view><view class="studyInput"><view class="title">资料详情:</view><input type="text" value="" name="studyInfo"/></view><view class="studyInput"><view class="title">资料类型:</view><picker mode="selector" name="studyType" value="{{index}}" range="{{arrayPicker}}" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view>{{arrayPicker[index]}}</view></picker></view><view class="studyInput"><view class="title">网盘地址:</view><input type="text" value="" name="studyAddress"/></view><view class="studyInput"><view class="title">提取码:</view><input type="text" value="" name="studyCode"/></view><view class="studyInput"><button form-type="submit" plain="true">发布</button></view></form></view></view>