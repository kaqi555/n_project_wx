<view class="moments data-v-090bcc30"><view class="moments__post data-v-090bcc30" id="{{'item'+index}}"><view class="post-left data-v-090bcc30"><image class="post_header data-v-090bcc30" src="{{headPic}}"></image></view><view class="post_right data-v-090bcc30"><text class="post-username data-v-090bcc30">{{username}}</text><view class="paragraph data-v-090bcc30" id="paragraph">{{content}}</view><view class="thumbnails data-v-090bcc30"><block wx:for="{{pics}}" wx:for-item="image" wx:for-index="index_images" wx:key="index_images"><view class="{{['data-v-090bcc30',pics.length===1?'my-gallery':'thumbnail']}}"><image class="gallery_img data-v-090bcc30" lazy-load="{{true}}" mode="aspectFill" src="{{image}}" data-src="{{image}}" data-event-opts="{{[['tap',[['previewImage',['$0',index_images],['pics']]]]]}}" bindtap="__e"></image></view></block></view><view class="toolbar data-v-090bcc30"><view class="timestamp data-v-090bcc30">{{time}}</view><view data-event-opts="{{[['tap',[['addLike',['$0'],['index']]]]]}}" class="like data-v-090bcc30" bindtap="__e"><image src="{{like?img.likeFill:img.like}}" class="data-v-090bcc30"></image></view><view data-event-opts="{{[['tap',[['$emit',['commentInput','$0'],['item']]]]]}}" class="comment data-v-090bcc30" bindtap="__e"><image src="{{img.talks}}" class="data-v-090bcc30"></image></view><view class="share data-v-090bcc30"><button open-type="share" plain="true" data-imgsrc="{{pics[0]}}" data-id="{{user_id}}" data-head="{{headPic}}" class="data-v-090bcc30"><image src="{{img.talkShare}}" class="data-v-090bcc30"></image></button></view><block wx:if="{{item.userId===item.user_id}}"><view class="delete data-v-090bcc30"><text class="data-v-090bcc30">删除</text></view></block></view><view class="post-footer data-v-090bcc30"><view class="footer_content data-v-090bcc30"><image class="liked data-v-090bcc30" src="{{img.isLiked}}"></image><block wx:for="{{likeName}}" wx:for-item="user" wx:for-index="index_like" wx:key="index_like"><text class="nickname data-v-090bcc30">{{user+'、'}}</text></block></view><block wx:for="{{item.talks}}" wx:for-item="comment" wx:for-index="comment_index" wx:key="comment_index"><view data-event-opts="{{[['tap',[['$emit',['commentUserInput','$0'],[[['item.talks','',comment_index]]]]]]]}}" class="footer_content data-v-090bcc30" bindtap="__e"><text class="comment-nickname data-v-090bcc30">{{comment.username+':'}}<text class="comment-content data-v-090bcc30">{{comment.content}}</text></text></view></block></view></view></view></view>